  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss|swf)$": "C:\\Users\\szymon\\WebstormProjects\\MelodyCloudReactClient\\tests\\__mocks__\\styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "C:\\Users\\szymon\\WebstormProjects\\MelodyCloudReactClient\\tests\\__mocks__\\fileMock.js"
